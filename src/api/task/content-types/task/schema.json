{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "todo",
        "inprogress",
        "done",
        "failed"
      ],
      "default": "todo",
      "required": true
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "urgent",
        "important",
        "normal"
      ],
      "default": "normal",
      "required": true
    },
    "level": {
      "type": "integer"
    },
    "estimation": {
      "type": "integer"
    },
    "beganAt": {
      "type": "datetime"
    },
    "finishedAt": {
      "type": "datetime"
    },
    "suspended": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "order": {
      "type": "integer",
      "default": 1,
      "required": true,
      "min": 1,
      "max": 10
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "link": {
      "displayName": "link",
      "type": "component",
      "repeatable": true,
      "component": "link.link"
    },
    "participant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::participant.participant",
      "inversedBy": "tasks"
    },
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event",
      "inversedBy": "task"
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "tasks"
    },
    "medal": {
      "type": "enumeration",
      "enum": [
        "light",
        "rocket",
        "jet"
      ]
    },
    "discussions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::discussion.discussion",
      "mappedBy": "task"
    },
    "specialty": {
      "displayName": "arrayOfString",
      "type": "component",
      "repeatable": true,
      "component": "basics.array-of-string"
    }
  }
}
