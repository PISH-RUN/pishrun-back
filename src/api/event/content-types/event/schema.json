{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "richtext"
    },
    "start_at": {
      "type": "datetime",
      "required": true
    },
    "end_at": {
      "type": "datetime",
      "required": true
    },
    "slug": {
      "type": "string",
      "regex": "^[a-zA-Z0-9\\-]*$",
      "required": true,
      "unique": true
    },
    "teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team.team",
      "mappedBy": "event"
    },
    "is_performing": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
