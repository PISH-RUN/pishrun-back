{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "mobile": {
      "type": "string",
      "unique": true,
      "regex": "^(\\+98|0)?9\\d{9}$"
    },
    "email": {
      "type": "email",
      "unique": true
    },
    "participants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::participant.participant",
      "mappedBy": "users_permissions_user"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "firstName": {
      "type": "string",
      "minLength": 3
    },
    "lastName": {
      "type": "string",
      "minLength": 3
    },
    "birthdate": {
      "type": "date"
    },
    "degree": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "behance": {
      "type": "string"
    },
    "otp": {
      "type": "string",
      "private": true
    },
    "otpSentAt": {
      "type": "datetime",
      "private": true
    },
    "otpExpiresAt": {
      "type": "datetime",
      "private": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
